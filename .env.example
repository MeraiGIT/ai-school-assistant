# Telegram API (from my.telegram.org)
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash
TELEGRAM_PHONE=+1234567890
TELEGRAM_SESSION=
TELEGRAM_2FA_PASSWORD=

# Device spoofing
DEVICE_MODEL=Desktop
SYSTEM_VERSION=Windows 10
APP_VERSION=4.16.8
LANG_CODE=ru
SYSTEM_LANG_CODE=ru-RU

# Supabase
SUPABASE_URL=https://your-project.supabase.co
# Use the service_role key (from Supabase Dashboard > Settings > API)
# This key bypasses RLS — never expose it to frontends
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# LLM
ANTHROPIC_API_KEY=your_anthropic_key
OPENAI_API_KEY=your_openai_key

# Letta Memory (optional — bot works without it)
# Get your key at https://app.letta.com/api-keys
LETTA_API_KEY=
# BYOK model: use your own LLM key (no Letta credits consumed)
# Add your OpenAI key at app.letta.com/models, then use: open-ai-api/gpt-4.1-mini
LETTA_BYOK_MODEL=open-ai-api/gpt-4.1-mini

# Admin authentication (optional — if not set, API is open for local dev)
# Set a random string for production: openssl rand -hex 32
ADMIN_API_KEY=

# App
LOG_LEVEL=INFO
FASTAPI_PORT=8000
